function smallerName(array) {
    let nameBook;
    
    array.forEach((element, i) => {
      let nameLenght = Object.keys(element.name);
      let lenght = 5;
      if (nameLenght.length < lenght){
        nameBook = element.name;
      }
    });
    return nameBook;
    
  }